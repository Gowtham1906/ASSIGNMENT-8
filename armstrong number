num = int(input("Enter a number: ")) 
l=int(input("Enter a last digit number: "))
sum = 0  
temp = num  
while temp > 0:  
   digit = temp % 10  
   sum += digit ** l
   temp //= 10  
if num == sum:  
   print("Given number is Armstrong number",num)  
else:  
   print("Given number is not Armstrong number",num)  
